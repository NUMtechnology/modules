{
  "@n": {
    "rewriteValue": "%/compact.@n"
  },
  "i": {
    "rewriteKey": "images",
    "arrayItems": "image"
  },
  "image": {
    "assignKeys": [
      "n",
      "t",
      "v"
    ],
    "replacePair": {
      "name": "%n",
      "type": "%t",
      "variants": "%v"
    }
  },
  "v": {
    "arrayItems": "variant"
  },
  "variant": {
    "assignKeys": [
      "u",
      "m",
      "w",
      "h"
    ],
    "replacePair": {
      "url": "%u",
      "mime": "%m",
      "width": "%w",
      "height": "%h"
    }
  }
}