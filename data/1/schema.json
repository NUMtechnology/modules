{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://modules.numprotocol.com/1/schema.json",
	"title": "Root",
	"type": "object",
	"definitions": {
		"@n": {
			"$id": "#root/@n",
			"title": "@n",
			"type": "integer",
			"default": 1
		},
		"link": {
			"$id": "#root/link",
			"title": "link",
			"type": "object",
			"default": {},
			"properties": {
				"@L": {
					"type": "string"
				},
				"name": {
					"type": "string"
				},
				"description": {
					"type": "string"
				},
				"object_display_name": {
					"type": "string"
				}
			},
			"required": [
				"@L"
			],
			"additionalProperties": false
		},
		"method": {
			"$id": "#root/method",
			"title": "method",
			"type": "object",
			"default": {},
			"properties": {
				"object_display_name": {
					"type": "string"
				},
				"value": {
					"type": "string"
				},
				"description": {
					"type": "string"
				},
				"hours": {
					"type": "object",
					"properties": {
						"available": {
							"type": "array",
							"items": {
								"type": "string"
							}
						},
						"time_zone_location": {
							"type": "string"
						}
					}
				},
				"object_type": {
					"type": "string"
				},
				"description_default": {
					"type": "string"
				},
				"prefix": {
					"type": "string"
				},
				"method_type": {
					"type": "string"
				}
			},
			"required": [
				"object_display_name"
			],
			"additionalProperties": false
		},
		"contact": {
			"$id": "#root/contact",
			"title": "contact",
			"type": "object",
			"default": {},
			"properties": {
				"link": {
					"$ref": "#root/link"
				},
				"telephone": {
					"$ref": "#root/method"
				}
			},
			"minItems": 1
		},
		"contacts": {
			"$id": "#root/contacts",
			"title": "contacts",
			"type": "array",
			"default": {},
			"items": {
				"$ref": "#root/contact"
			},
			"minItems": 1
		},
		"entity": {
			"$id": "#root/entity",
			"title": "entity",
			"type": "object",
			"default": {},
			"required": [
				"name",
				"contacts"
			],
			"properties": {
				"name": {
					"type": "string"
				},
				"contacts": {
					"$ref": "#root/contacts"
				},
				"object_type": {
					"type": "string"
				},
				"object_display_name": {
					"type": "string"
				},
				"description_default": {
					"type": "string"
				}
			},
			"additionalProperties": false
		},
		"person": {
			"$id": "#root/person",
			"title": "person",
			"type": "object",
			"default": {},
			"required": [
				"name",
				"contacts"
			],
			"properties": {
				"name": {
					"type": "string"
				},
				"contacts": {
					"$ref": "#root/contacts"
				},
				"object_type": {
					"type": "string"
				},
				"object_display_name": {
					"type": "string"
				},
				"description_default": {
					"type": "string"
				},
				"bio": {
					"type": "string"
				}
			},
			"additionalProperties": false
		},
		"organisation": {
			"$id": "#root/organisation",
			"title": "organisation",
			"type": "object",
			"default": {},
			"required": [
				"name",
				"contacts"
			],
			"properties": {
				"name": {
					"type": "string"
				},
				"contacts": {
					"$ref": "#root/contacts"
				},
				"object_type": {
					"type": "string"
				},
				"object_display_name": {
					"type": "string"
				},
				"description_default": {
					"type": "string"
				},
				"slogan": {
					"type": "string"
				}
			},
			"additionalProperties": false
		}
	},
	"required": [
		"@n"
	],
	"properties": {
		"@n": {
			"$ref": "#root/@n"
		},
		"@p": {
			"type": "string"
		},
		"organisation": {
			"$ref": "#root/organisation"
		},
		"department": {
			"$ref": "#root/entity"
		},
		"group": {
			"$ref": "#root/entity"
		},
		"person": {
			"$ref": "#root/person"
		}
	},
	"additionalProperties": false
}