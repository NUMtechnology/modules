			"$id": "#root/notNullString",
			"title": "notNullString",
			"type": "string",
			"nullable": false
		},
		"notEmptyString": {
			"$id": "#root/notEmptyString",
			"title": "notEmptyString",
			"type": "string",
			"minLength": 1,
			"nullable": false
		},
		"hours": {
			"$id": "#root/hours",
			"title": "hours",
